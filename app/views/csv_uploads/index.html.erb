<iframe src="<%= ENV['MOVIE'] %>" width="640" height="480"></iframe>
<iframe src="<%= ENV['SLIDE']%>" frameborder="0" width="960" height="569" allowfullscreen="true" mozallowfullscreen="true" webkitallowfullscreen="true"></iframe>

<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>タイトル</th>
      <th>項目</th>
    </tr>
  </thead>

  <tbody>
    <% @csv_uploads.each do |csv| %>
      <tr>
        <td><%= csv.id %></td>
        <td><%= csv.title %></td>
        <td><%= csv.content %></td>
        <td><%= link_to "エクスポート", csv_export_csv_upload_path(id: csv.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br/>

<%= form_tag csv_import_csv_uploads_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "インポート" %>
<% end %>
